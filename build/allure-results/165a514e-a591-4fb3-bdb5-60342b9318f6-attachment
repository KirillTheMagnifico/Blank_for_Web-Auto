    <script nonce="">
  window.on_paginate = function(el, url, type) {
    document.querySelector('.loadLink')?.classList.add('loader_block');
    fetch(url, {
      method: 'POST',
      headers: {
        'Content-Type': 'application/json;charset=utf-8',
        'X-CSRF-TOKEN': document.querySelector('meta[name="csrf-token"]').getAttribute('content'),
        'X-Requested-With': 'XMLHttpRequest',
      },
    })
    .then(response => response.json())
    .then(data => {
        const block = document.querySelector('.loadLink')?.closest('.for_paginate');
        block.querySelector('.pagination-block')?.remove();

        if (type === 'more') {
          block.innerHTML = block.innerHTML + data.partial;
        } else {
          block.innerHTML = data.partial;
        }
        setTimeout(() => {
          document.querySelectorAll('.owl-carousel').forEach(carousel => {
            carousel.style.display = 'block'
            carousel.querySelectorAll('.item:not(:first-child)').forEach(item => {
              item.style.display = 'none'
            })
          })
        }, 500)
        if (data.url) {
          let url = decodeURIComponent(data.url);
          history.pushState(url, "", url);
        }
       document.querySelector('.loadLink')?.classList.remove('loader_block');
    })
  }
</script>
