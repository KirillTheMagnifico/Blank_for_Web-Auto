            s.charset = 'utf-8';
            s.async = true;
            s.defer = true;
            s.src = '//me-talk.ru/support/support.js?h=1645ced87d2f67dd699696c718a5dfdd';
            var sc = document.getElementsByTagName('script')[0];
            var callback = function () {/*     API. ,     : supportAPI.setSupportTop(200); */
            };
            s.onreadystatechange = s.onload = function () {
                var state = s.readyState;
                if (!callback.done && (!state || /loaded|complete/.test(state))) {
                    callback.done = true;
                    callback();
                }
            };
            if (sc) sc.parentNode.insertBefore(s, sc); else document.documentElement.firstChild.appendChild(s);
        })();</script>


