                            <script nonce="" type="text/javascript">

      function setCookie(name, val) {
        // Set a cookie named "cookieName" with a value of "cookieValue" and an expiration date of 7 days
        var expires = ""
        var date = new Date()
        date.setTime(date.getTime() + (7 * 24 * 60 * 60 * 1000))
        expires = "; expires=" + date.toUTCString()
        document.cookie = name + "=" + (val || "") + expires + "; path=/"
      }

      function removeCookie(name) {
        document.cookie = name + "=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;"
      }

      window.onload = function () {
        if ($("#yandex_oauth").length) {
          if (!document.cookie.includes("auth-popup-seen")) {
            if (document.cookie.includes("preffer_auth=yandex")) {
              yandexPopupInit()
            } else {
              initAuthPopups()
            }
          } else {
            console.log("skip auth popup")
          }

          function yandexPopupInit() {
            setCookie("auth-popup-seen", "1")

            if ($("#yandex_oauth").length) {
              const {client_id, base_url, redirect} = JSON.parse($("#yandex_oauth").val())

              console.log("yandex oauth init")
              YaAuthSuggest.init({
                  client_id: client_id,
                  response_type: "token",
                  redirect_uri: redirect,
                },
                base_url,
              ).then(({handler}) => handler()).then(data => {
                $.ajax({
                  url: "/ajax/social/yandex",
                  method: "POST",
                  data: data,
                }).done(function (data) {
                  console.log(data)
                  setCookie("preffer_auth", "yandex")
                  removeCookie("auth-popup-seen")
                  window.location.reload()
                })
                console.log("  ", data)
              }).catch(error => console.log(" ", error))

            }
          }

          function initAuthPopups() {

            const {client_id} = JSON.parse($("#google_oauth").val())

            google.accounts.id.initialize({
              client_id: client_id,
              callback: handleCredentialResponse,
            })

            google.accounts.id.prompt((notification) => {
              console.log("google callback")

              setCookie("auth-popup-seen", "1")

              if (notification.isNotDisplayed()) {
                // try next provider if OneTap is not displayed or skipped
                yandexPopupInit()
              }
            })

            function handleCredentialResponse(response) {
              $.post("/ajax/auth/google/one-tap/callback", response, function (response) {
                // Success callback function
                console.log("Success:")
                setCookie("preffer_auth", "google")
                removeCookie("auth-popup-seen")

                window.location.reload()
              }).fail(function () {
                // Error callback function
                console.log("Error")
              })
              console.log(response)
            }
          }
        }

      }
    </script>
                                <script nonce="">
                                    var streams_html = `
                                    <div class="owl-carousel broadcast-box">
                                                                                    <div class="item">
                                                <iframe width="406" height="231" src="about:blank" frameborder="0"></iframe>
                                            </div>
                                                                                    <div class="item">
                                                <iframe width="406" height="231" src="about:blank" frameborder="0"></iframe>
                                            </div>
                                                                                    <div class="item">
                                                <iframe width="406" height="231" src="about:blank" frameborder="0"></iframe>
                                            </div>
                                                                            </div>`;

                                    const streams = [
                                        '<iframe width="406" height="231" src="https://rtsp.me/embed/Yn24e69f/" frameborder="0"></iframe>','<iframe width="406" height="231" src="https://rtsp.me/embed/z9rtEGtd/" frameborder="0"></iframe>','<iframe width="406" height="231" src="https://rtsp.me/embed/AB3Rf9DR/" frameborder="0"></iframe>',                                    ];

                                    function showStream() {
                                        $.ajax({
                                            url: '/ajax/watch_stream',
                                            method: 'GET',
                                        }).done(function (data) {
                                            console.log('stream watched')
                                        });

                                        $(".button_play_big").remove();

                                        $('.video-wrap').append($(streams_html));
                                        var broadcastBox = $('.broadcast-box');

                                        if (broadcastBox.length) {
                                            broadcastBox.owlCarousel({
                                                loop:false,
                                                margin:8,
                                                nav:true,
                                                items:broadcastBox.length,
                                                lazyLoad:false,
                                                center:true,
                                                dots: true,
                                            });

                                            broadcastBox.appendTo('.video-wrap');

                                            //  iframe'  
                                            $('.broadcast-box').find('iframe').each(function (index) {
                                                $(this).attr('src', $(streams[index]).attr('src'));
                                            })
                                        }

                                        $('#preview_btn').remove();
                                    }
                                </script>
                            
